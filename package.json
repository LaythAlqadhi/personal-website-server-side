{
  "name": "express app",
  "version": "0.0.0",
  "engines": {
    "node": ">=20.10.0"
  },
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "node ./bin/www.js",
    "devstart": "nodemon ./bin/www.js",
    "serverstart": "DEBUG=* npm run devstart",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "all",
    "printWidth": 80
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:node/recommended",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "commonjs": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "no-console": "off",
      "consistent-return": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^next$"
        }
      ]
    },
    "ignorePatterns": [
      "www.js",
      "node_modules/"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^9.1.3",
    "mongoose": "^8.0.1",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  }
}
